{
  "include-component-in-tag": true,
  "include-v-in-tag": true,
  "packages": {
    "ansible": {
      "extra-files": [
        {
          "jsonpath": "$.version",
          "path": "galaxy.yml",
          "type": "yaml"
        }
      ],
      "package-name": "ansible-collection",
      "release-type": "simple"
    },
    "kustomization/components/priorityclass": {
      "package-name": "kustomize-priorityclass",
      "release-type": "simple"
    },
    "kustomization/components/unifi-network-application": {
      "package-name": "kustomize-unifi-network-application",
      "release-type": "simple"
    }
  },
  "separate-pull-requests": true,
  "tag-separator": "@",
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
